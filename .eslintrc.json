{
	"extends": ["eslint:recommended", "plugin:prettier/recommended"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"arrow-body-style": "off",
		"prefer-arrow-callback": "off",
		"no-unused-vars": [
			"error", 
			{ "args": "none" }
		]
	},
	"env": {
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 11
	}
}
